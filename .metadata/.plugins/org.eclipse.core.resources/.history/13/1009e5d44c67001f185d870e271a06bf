event PosicionMesa {

	val posActual : int
	val posDeseada : int

	new (i : int , j: int) {
		posActual = i
		posDeseada = j
	}

}

event PosicionCuboA {

	val posActual : int
	val posDeseada : int

	new (i : int, j : int) {
		posActual = i
		posDeseada = j
	}

}

event PosicionCuboB {

	val posActual : int
	val posDeseada : int

	new (i : int, j : int) {
		posActual = i
		posDeseada = j
	}

}

event PosicionCuboC {

	val posActual : int
	val posDeseada : int

	new (i : int, j : int) {
		posActual = i
		posDeseada = j
	}

}

event PedirPosicion{
	
}

event CambiarPosicionACuboA {
	val posActual : int

	new (i : int) {
		posActual = i
	}
}

event CambiarPosicionACuboB {
	val posActual : int

	new (i : int) {
		posActual = i
	}
}

event CambiarPosicionACuboC {
	val posActual : int

	new (i : int) {
		posActual = i
	}
}


